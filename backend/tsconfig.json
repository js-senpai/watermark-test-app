{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [ "webworker", "es6", "dom" ],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "declaration": false,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types",
      "./typings"
    ]
  }
}